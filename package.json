{
  "name": "e-commerce_MERN_project",
  "version": "1.0.0",
  "description": "MERN ecommerce app",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"yarn run server\" \"yarn run client\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d"
  },
  "repository": "git@github.com:YusukeYoshihiro/e-commerce_MERN_project.git",
  "author": "Yusuke Yoshihiro",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "concurrently": "^6.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "mongoose": "^5.11.18",
    "nodemon": "^2.0.7"
  }
}
